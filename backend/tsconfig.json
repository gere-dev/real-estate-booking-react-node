{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true, 
        "resolveJsonModule": true, 
        "sourceMap": true,
        "baseUrl": "./src", 
        "paths": {
            "@/*":["*"],
            "@db/*": ["database/config/*"],
            "@controllers":["controllers/*"],
            "@routes/*": ["routes/*"],
            "@models/*": ["database/models/*"]
        }
    }

}